	  "noImplicitAny": true,
	  "removeComments": false,
	  "preserveConstEnums": true,
	  "sourceMap": false,
	  "target": "ESNext",
	  "esModuleInterop": true,
	  "jsx": "react",
	  "outDir": "dist",
	  "declaration": true,
	  "moduleResolution": "NodeNext"
	},
	"include": ["./src/**/*", "Invoker"]
